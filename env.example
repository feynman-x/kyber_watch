# KyberSwap Earn API base URL
KYBER_EARN_API_BASE_URL=https://earn-service.kyberswap.com/api/v1/explorer/pools

# Comma-separated chain IDs (default: 8453,56)
KYBER_CHAIN_IDS=8453,56

# Number of pages to fetch (default: 5)
POOL_PAGES=5

# Page size per request (default: 10)
POOL_LIMIT=10

# Minimum APR filter (default: 0)
POOL_MIN_APR=0

# Minimum earn fee filter (default: 0)
POOL_MIN_EARN_FEE=0

# Minimum volume filter (default: 0)
POOL_MIN_VOLUME=0

# Lark webhook URL for notifications
LARK_WEBHOOK_URL=

# Notification cooldown per pool in ms (default: 259200000)
POOL_NOTIFY_COOLDOWN_MS=259200000

# File path to persist notification timestamps (default: data/pool-notify.json)
POOL_NOTIFY_STORE_PATH=data/pool-notify.json
